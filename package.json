{
  "name": "panamerican-taekwondo-academy",
  "version": "1.0.0",
  "description": "Official website for Panamerican Taekwondo Academy in Winston-Salem, NC",
  "main": "src/js/main.js",
  "scripts": {
    "build": "npm run build:js",
    "build:js": "uglifyjs src/js/main.js -o src/js/main.min.js -c -m",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint src/js/**/*.js",
    "lint:css": "stylelint src/css/**/*.css",
    "format": "prettier --write \"src/**/*.{js,css,html}\"",
    "deploy": "gh-pages -d src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MartialArtsCode/panamerican-taekwondo-website.git"
  },
  "keywords": [
    "taekwondo",
    "martial-arts",
    "winston-salem",
    "academy",
    "wordpress",
    "responsive"
  ],
  "author": "Carlos Padilla - Panamerican Taekwondo Academy",
  "license": "MIT",
  "devDependencies": {
    "uglify-js": "^3.17.4",
    "eslint": "^8.57.0",
    "stylelint": "^16.0.0",
    "stylelint-config-standard": "^36.0.0",
    "prettier": "^3.0.0",
    "gh-pages": "^6.0.0"
  }
}
